{
  "name": "StopCOVIDNI",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test:ios": "detox test --configuration=ios",
    "test:android": "detox test --configuration=ios",
    "pre:test:ios": "detox build --configuration=ios",
    "pre:test:android": "detox build --configuration=ios",
    "typecheck": "tsc -p tsconfig.json",
    "postinstall": "./postinstall.sh"
  },
  "dependencies": {
    "react-native-exposure-notification-service": "1.1.15",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.4",
    "@react-native-community/push-notification-ios": "^1.3.0",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/native": "^5.7.0",
    "@react-navigation/stack": "^5.6.2",
    "art": "^0.10.3",
    "babel-plugin-dotenv-import": "^2.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "date-fns": "^2.14.0",
    "expo-asset": "^8.1.5",
    "expo-crypto": "^8.2.1",
    "expo-font": "^8.2.1",
    "expo-haptics": "^8.2.1",
    "expo-intent-launcher": "^8.2.1",
    "expo-localization": "^8.2.1",
    "expo-secure-store": "^9.0.1",
    "expo-web-browser": "^8.3.1",
    "formik": "^2.1.4",
    "i18next": "^19.5.6",
    "react": "16.13.1",
    "react-i18next": "^11.7.0",
    "react-native": "0.62.2",
    "react-native-dotenv": "^2.2.0",
    "react-native-easy-markdown": "^2.0.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-google-safetynet": "^1.0.0",
    "react-native-ios11-devicecheck": "^0.0.3",
    "react-native-loading-spinner-overlay": "^1.1.0",
    "react-native-modal": "^11.5.6",
    "react-native-permissions": "^2.1.5",
    "react-native-push-notification": "^3.5.2",
    "react-native-safe-area-context": "^3.1.1",
    "react-native-screens": "^2.8.0",
    "react-native-ssl-pinning": "^1.4.8",
    "react-native-svg": "^12.1.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-unimodules": "^0.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/detox": "^16.4.1",
    "@types/jasmine": "^3.5.10",
    "@types/jest": "^26.0.4",
    "@types/react-native": "^0.63.1",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-native-loading-spinner-overlay": "^0.5.2",
    "@types/react-native-push-notification": "^3.0.9",
    "@types/react-native-svg-charts": "^5.0.3",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-jest": "^26.1.0",
    "detox": "^17.1.1",
    "eslint": "^7.4.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-react-native-a11y": "^2.0.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "lint-staged": "^10.2.11",
    "metro-react-native-babel-preset": "^0.60.0",
    "prettier": "^2.0.4",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.6"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{css,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && lint-staged"
    }
  }
}
